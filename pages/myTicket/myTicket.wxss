.header {
  background-color: #eeeef0;
}
.tab {
  background-color: #006400;
  text-align: center;
  font-size: 26rpx;
  color: #eaeaea;
  font-weight: 200;
  padding: 5rpx 0 15rpx 0;
}
.tabActive {
  color: #ffffff;
  font-weight: 350;
  background-color: #003300;
  width: 70rpx;
  padding: 4rpx 20rpx;
  line-height: 30rpx;
  border-radius: 20rpx;
}
.workTime {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
}
.worlTimeItem {
  height: 100rpx;
  width: 94%;
  background-color: #ffffff;
  border-style: solid;
  border-color: #006400;
  border-width: 2rpx;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
}
.bag {
  margin-left: 16rpx;
}
.workTimeText {
  font-size: 26rpx;
  line-height: 26rpx;
  margin-bottom: 8rpx;
  margin-left: 50rpx;
}
.workTimeArrow {
  margin-bottom: 8rpx;
  margin-left: 60rpx;
}
.main {
  background-color: #eeeef0;
  flex: 1;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow-y: auto;
}
.main-scroll {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 94%;
}
.main-ticket {
  background-color: #ffffff;
  height: 200rpx;
  position: relative;
  margin: 20rpx 0;
  border-radius: 10rpx;
}
.main-ticket::before {
  content: ' ';
  width: 30rpx;
  height: 20rpx;
  border-radius: 40%;
  background-color: #eaeaea;
  position: absolute;
  left: -20rpx;
  top: 60%;
}
.main-ticket::after {
  content: ' ';
  width: 30rpx;
  height: 20rpx;
  border-radius: 40%;
  background-color: #eaeaea;
  position: absolute;
  right: -20rpx;
  top: 60%;
}
.swipe-right{
  text-align: center;
  top: 25%;
position: relative;

}
hr {
  position: absolute;
  top: 64%;
  border: 1rpx solid #eaeaea;
  width: 90%;
  left: 5%;
}
.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}
.fixedButton {
  background-color: transparent;
  display: flex;
  justify-content: space-around;
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  line-height: 100rpx;
}
.button {
  width: 250rpx;
  border-radius: 50rpx;
  display: inline-block;
  font-size: 36rpx;
  color: #eaeaea;
  font-weight: 300;
}
.QRcode {
  background-color: #006400;
}
.BuyTicket {
  background-color: #cc7b38;
}
.qr {
  position: relative;
  top: 5rpx;
}
.coupon {
  position: relative;
  top: 5rpx;
  transform: rotate(130deg);
  margin-right: 8rpx;
}
